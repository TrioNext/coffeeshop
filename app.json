{
  "expo": {
    "name": "King Kong Tea Milk",
    "slug": "coffeeshop",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "notification": {
      "icon": "./assets/images/notification.png"
    },

    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#3b853f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "ios": {
      "bundleIdentifier": "com.honghai.coffeeshop",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
      
    },
    "android": {
      "package": "com.honghai.coffeeshop"
    },
    "description": ""
  }
}
